@model List<DDAC.Models.CustomerModels>
@{
    ViewBag.Title = "View Customer";
}

<style>
    table, th, td {
        border: 1px solid white;
        border-collapse: collapse;
    }

    th, td {
        padding: 5px;
    }
</style>

<div class="container-fluid">
    <h2>ViewCustomer</h2>

    <table id="customer" style="width: 100%">
        <tr>
            <th>Customer Name</th>
            <th>Ic No</th>
            <th>Phone Number</th>
            <th>Email</th>
            <th>Registered By</th>
        </tr>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.CustomerName</td>
                <td>@i.IcNo</td>
                <td>@i.PhoneNumber</td>
                <td>@i.Email</td>
                <td>@i.AspNetUsersId</td>
            </tr>
        }
    </table>
</div>

@section scripts
{

    <script>
        $(document).ready(function() {
        });
    </script>

}

